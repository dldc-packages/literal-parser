{
  "name": "literal-parser",
  "version": "2.1.0",
  "description": "A small library to parse JavaScript array/object literal",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/literal-parser#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/literal-parser/issues"
