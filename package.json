{
  "name": "literal-parser",
  "version": "3.0.2",
  "description": "A small library to parse JavaScript array/object literal",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/literal-parser#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/literal-parser/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/literal-parser.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js"
    }
  },
  "main": "./dist/mod.js",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist deno_dist && tsup --format cjs,esm src/mod.ts --dts src/mod.ts && denoify",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "denoify": "^1.4.10",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.2",
    "np": "^7.6.3",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
