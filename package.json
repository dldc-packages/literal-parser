  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "husky": "^4.3.0",
