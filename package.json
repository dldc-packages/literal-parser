  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/literal-parser.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "files": [
    "dist",
    "bin"
  ],
  "main": "dist/index.js",
  "module": "dist/literal-parser.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "build": "tsdx build",
