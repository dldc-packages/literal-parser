{
  "name": "literal-parser",
  "version": "2.1.1",
  "description": "A small library to parse JavaScript array/object literal",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/literal-parser#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/literal-parser/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/literal-parser.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "files": [
    "dist",
    "bin"
  ],
  "main": "dist/index.js",
  "module": "dist/literal-parser.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "build": "tsdx build",
    "lint": "tsdx lint src test",
    "start": "tsdx watch",
    "test": "tsdx test --coverage",
    "test:watch": "tsdx test --watch",
    "typecheck": "tsc --noEmit",
    "pub": "yarn build && np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "settings": {
      "react": {
        "version": "999.999.999"
      }
    },
    "rules": {
      "no-redeclare": 0
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "husky": "^4.3.8",
    "np": "^7.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  }
}
